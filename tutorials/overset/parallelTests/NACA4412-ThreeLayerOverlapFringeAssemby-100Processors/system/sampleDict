FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;
surfaceFormat raw;

setFormat   gnuplot;

sets ();

fields (p);

surfaces
(
    solidWall
    {
        type        patch;
        patchName   airfoil;
        rhoName rhoInf;
        rhoInf 1.0; //Reference density for fluid
        outputInterval: 1.0
        interpolate    true;
        triangulate    false;
    }
);


