FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions [0 2 -1 0 0 0 0];

internalField uniform 0;

boundaryField
{
    OversetFaces
    {
        type    overset;
        value   uniform 0;

        coupledFringe yes;

        setHoleCellValue  yes;
        holeCellValue     0;
    }

    Inflow
    {
        type    calculated;
        value   uniform 0;
    }
    Outflow
    {
        type    calculated;
        value   uniform 0;
    }

    Lower
    {
        type    calculated;
        value   uniform 0;
    }
    Upper
    {
        type    calculated;
        value   uniform 0;
    }

    frontAndBack
    {
        type    empty;
    }

    airfoil
    {
        type    nutkWallFunction;
        Cmu     0.09;
        kappa   0.41;
        E       9.8;
        value   uniform 0;
    }

    OversetMiddle
    {
        type    emptyOverset;
        value   uniform 0;
    }
    OversetFront
    {
        type    emptyOverset;
        value   uniform 0;
    }
}


