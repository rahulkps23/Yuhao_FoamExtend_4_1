FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          CG;
        preconditioner  Cholesky;
        tolerance       1e-06;
        relTol          0;
    }
    U
    {
        solver          BiCGStab;
        preconditioner  DILU;
        tolerance       1e-05;
        relTol          0;
    }
}

PIMPLE
{
    nOuterCorrectors            4;
    nCorrectors                 2;
    nNonOrthogonalCorrectors    0;
    pRefPoint (0.5 1.5 0);
    pRefValue 0;
}


