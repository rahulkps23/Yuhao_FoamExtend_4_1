FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 500;

boundaryField
{
    ibCylinder
    {
        type            immersedBoundaryOmegaWallFunction;
        patchType       immersedBoundary;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;

        value           uniform 500;
    }
    top
    {
        type            zeroGradient;
    }
    in
    {
        type            fixedValue;
        value           uniform 500;
    }
    out
    {
        type            inletOutlet;
        inletValue      uniform 500;
        value           uniform 500;
    }
    bottom
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


