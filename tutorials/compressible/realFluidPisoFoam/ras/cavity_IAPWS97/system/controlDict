FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs
(
    "libcompressibleRASModels.so"
)

application     realFluidPisoFoam;

startFrom       latestTime;

//startTime       0;

stopAt          endTime;

endTime         0.2;

deltaT          1e-5;

writeControl    runTime;

writeInterval  1e-2;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   10;

adjustTimeStep  yes;

maxCo          0.75;

maxDeltaT       0.01;

runTimeModifiable yes;


