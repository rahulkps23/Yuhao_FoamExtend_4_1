FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inlet
    {
        type            isentropicTotalTemperature;
        phi             phi;
        rho             none;
        psi             psi;
        U               Uabs;
        gamma           1.4;
        T0              uniform 300;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    blade
    {
        type            zeroGradient;
    }

    shaft
    {
        type            zeroGradient;
    }

    shroud
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            cyclic;
    }
}


// ************************************************************************* //
