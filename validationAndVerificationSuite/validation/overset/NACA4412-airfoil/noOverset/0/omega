FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.42797;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    inlet
    {
        type            inletOutlet;
        inletValue      uniform 0.42797;
        value           uniform 0.42797;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.42797;
        value           uniform 0.42797;
    }
    solidWall
    {
        type            omegaWallFunction;
        refValue        uniform 0;
        value           uniform 0.42797;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        beta1           0.075;
    }
}


